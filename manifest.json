{
	"name": "BrowserExtTest",
	"version": "1.0",
	"manifest_version": 2,
	"description": "Chome extension test",
	"icons": {
		"128": "icons/seevl_128.png",
		"16": "icons/seevl_16.png",
		"48": "icons/seevl_48.png"
